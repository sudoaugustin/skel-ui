{
  "compilerOptions": {
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2018",
    "module": "CommonJS",
    "declaration": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
